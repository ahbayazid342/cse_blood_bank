<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.childrenschool">

    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ChildrenSchool">
        <activity android:name=".BloodBank.viewDetails.ViewDetails"></activity>
        <activity android:name=".BloodBank.addDonor.AddDonorDetails" />
        <activity android:name=".BloodBank.BloodCollection" />
        <activity android:name=".uploadactivity.ImageActivity" />
        <activity android:name=".uploadactivity.UploadAlphabetActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>